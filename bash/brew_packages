# brew installer helper, normally run this from 'dot'
# check: http://docs.python-guide.org/en/latest/starting/install/osx/
TMPPATH=$PATH
PATH=/usr/local/bin:$PATH

brew install bash-completion@2 || brew upgrade bash-completion@2
brew install zoxide || brew upgrade zoxide

export PATH=$TMPPATH
